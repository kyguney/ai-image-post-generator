{
  "title": "Unsplash Card",
  "description": "Include unsplash image with content",
  "textdomain": "aipg-plugin",
  "name": "aipg-plugin/unsplash-card",
  "icon": "media-and-text",
  "apiVersion": 2,
  "category": "aipg-plugin",
  "attributes": {
    "align": {
      "type": "string",
      "default": "wide"
    },
    "verticalAlign": {
      "type": "string"
    },
    "keywords": {
      "type": "array"
    },
    "autoSearch": {
      "type": "boolean"
    },
    "content": {
      "type": "string"
    },
    "imagePosition": {
      "type": "string",
      "default": "left"
    }
  },
  "supports": {
    "align": ["full", "wide"]
  },
  "editorScript": "file:./index.js"
}
